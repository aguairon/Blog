<header class="header period-header">
  <div id="sidebarCollapse" class="btn btn-info navbar-btn">
    <i class="fa fa-bars"></i>
  </div>
  <h1 class="main">Periodos historicos
    <% if admin_signed_in? %>
      <%= link_to 'Nuevo periodo', new_period_path, class:"text-muted" %>
    <% end %>
  </h1>
</header>

<div class="list periods_list">
  <% @periods.each do |period| %>
    <div><%= link_to period.title, period_path(period) %></div>
  <% end %>
</div>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>